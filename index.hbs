{{!< default}}
<div id="page" class="hfeed site grid-container container grid-parent">
  <div id="content" class="site-content">
    <div id="primary" class="content-area grid-parent">
      <main class="site-main" role="main">
      <!-- This is the post loop - each post will be output using this markup -->
        {{#foreach posts}}
        <article class="{{post_class}}" itemtype="https://schema.org/CreativeWork" itemscope>
          <div class="inside-article">
            <header class="post-header">
              <h2 class="post-title"><a href="{{url}}">{{title}}</a></h2>
            </header>
            <footer class="post-meta">
              <time class="post-date" datetime="{{date format='YYYY-MM-DD'}}">{{date format="YYYY-MM-DD"}}</time>
              <span class="by-line" itemtype="https://schema.org/Person" itemscope>
                by
                {{author.name}}
              </span>
            </footer>
            <section class="entry-summary" itemprop="text">
              <p class="post-content">{{content}}</p>
            </section>
            <div class="post-meta">
              <svg t="1544338715036" class="icon-tag" style="" viewBox="0 0 1097 1024" version="1.1" xmlns="http://www.w3.org/2000/svg" p-id="3080" xmlns:xlink="http://www.w3.org/1999/xlink" width="214.2578125" height="200"><defs><style type="text/css"></style></defs><path d="M256 256c0-40.557714-32.585143-73.142857-73.142857-73.142857s-73.142857 32.585143-73.142857 73.142857 32.585143 73.142857 73.142857 73.142857 73.142857-32.585143 73.142857-73.142857z m609.718857 329.142857c0 19.419429-8.009143 38.290286-21.138286 51.419429l-280.576 281.161143c-13.714286 13.129143-32.585143 21.138286-52.004571 21.138285s-38.290286-8.009143-51.419429-21.138285L52.004571 508.562286C22.857143 480 0 424.557714 0 384V146.285714c0-40.009143 33.133714-73.142857 73.142857-73.142857h237.714286c40.557714 0 96 22.857143 125.147428 52.004572l408.576 407.990857c13.129143 13.714286 21.138286 32.585143 21.138286 52.004571z m219.428572 0c0 19.419429-8.009143 38.290286-21.138286 51.419429l-280.576 281.161143a76.068571 76.068571 0 0 1-52.004572 21.138285c-29.696 0-44.580571-13.714286-64-33.718857l268.580572-268.580571c13.129143-13.129143 21.138286-32 21.138286-51.419429s-8.009143-38.290286-21.138286-52.004571L527.433143 125.147429C498.285714 96 442.88 73.142857 402.285714 73.142857h128c40.557714 0 96 22.857143 125.147429 52.004572l408.576 407.990857c13.129143 13.714286 21.138286 32.585143 21.138286 52.004571z" fill="" p-id="3081"></path></svg>
              {{tags}}
            </div>
          </div>
        </article>
        {{/foreach}}
      </main>
    </div>
  </div>
</div>

<!-- Previous/next page links - displayed on every page -->
{{pagination}}